<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="YupItsZac\FreeGeoBundle\Entity\Apps" table="apps">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="apptitle" type="string" column="AppTitle" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="appdescription" type="string" column="AppDescription" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="appwebsite" type="string" column="AppWebsite" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="publickey" type="string" column="PublicKey" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secretkey" type="string" column="SecretKey" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="Status" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="beta" type="integer" column="Beta" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="assoc" type="integer" column="Assoc" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hash" type="string" column="Hash" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
