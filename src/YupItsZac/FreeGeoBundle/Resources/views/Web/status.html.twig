{% extends 'base.html.twig' %}

{% block body %}
   
   <div class="row mbot">
		<div class="content col-md-8 col-md-offset-2">

		<h1>API Status Report</h1>

			{% for key,value in servicesStatus %}
				<b>Service</b>: {{ key }}
				<b>Status</b> : {{ value.Status }}
				<b>Endpoint</b>: {{ value.Endpoint }}
				<br><br>
			{% endfor %}

		</div>

	</div>


{% endblock %}
